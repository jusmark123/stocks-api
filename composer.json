{
  "type": "project",
  "license": "proprietary",
  "repositories": {
    "local": {
      "type": "path",
      "url": "./repos/*",
      "options": {
        "symlink": true
      }
    },
    "packagist": {
      "type": "composer",
      "url": "https://repo.packagist.org/"
    }
  },
  "require": {
    "php": ">=7.2.5",
    "ext-amqp": "*",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-xdebug": "*",
    "ext-sodium": "*",
    "api-platform/core": "^2.5",
    "bunny/bunny": "^0.4.3",
    "composer/package-versions-deprecated": "1.11.99.1",
    "dg/bypass-finals": "^1.3",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.2",
    "doctrine/doctrine-migrations-bundle": "^3.0",
    "doctrine/orm": "^2.8",
    "evenement/evenement": "^3.0",
    "gbprod/uuid-normalizer": "^1.2",
    "guzzlehttp/promises": "^1.4",
    "guzzlehttp/psr7": "^1.7",
    "lexik/jwt-authentication-bundle": "^2.6",
    "gesdinet/jwt-refresh-token-bundle": "v0.10.1",
    "nelmio/cors-bundle": "^2.1",
    "nyholm/psr7": "^1.3",
    "php-http/client-common": "^2.3",
    "php-http/message": "^1.10",
    "phpdocumentor/reflection-docblock": "^5.2",
    "polygon-io/api": "^1.0",
    "predis/predis": "^1.1",
    "ramsey/uuid": "^4.1",
    "ramsey/uuid-doctrine": "^1.6",
    "react/child-process": "^0.6.1",
    "snc/redis-bundle": "^3.2",
    "stof/doctrine-extensions-bundle": "^1.5",
    "symfony/amqp-messenger": "5.1.*",
    "symfony/asset": "5.1.*",
    "symfony/console": "5.1.*",
    "symfony/dotenv": "5.1.*",
    "symfony/expression-language": "5.1.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.1.*",
    "symfony/mailer": "5.1.*",
    "symfony/monolog-bundle": "^3.6",
    "symfony/notifier": "5.1.*",
    "symfony/property-access": "5.1.*",
    "symfony/property-info": "5.1.*",
    "symfony/security-bundle": "5.1.*",
    "symfony/http-client": "5.1.*",
    "symfony/serializer": "5.1.*",
    "symfony/twig-bridge": "5.1.*",
    "symfony/twig-bundle": "5.1.*",
    "symfony/validator": "5.1.*",
    "tansoft/websocketclient": "^1.0"
  },
  "require-dev": {
    "behat/behat": "^3.8",
    "behatch/contexts": "^3.3",
    "doctrine/data-fixtures": "^1.4",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "friends-of-behat/mink-browserkit-driver": "^1.4",
    "friends-of-behat/symfony-extension": "^2.1",
    "friendsofphp/php-cs-fixer": "^2.16",
    "phake/phake": "^3.1",
    "symfony/debug-bundle": "5.1.*",
    "symfony/phpunit-bridge": "^5.1",
    "symfony/stopwatch": "^5.1",
    "symfony/swiftmailer-bundle": "^3.5",
    "symfony/web-profiler-bundle": "^5.1",
    "symfony/maker-bundle": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "make build-cache": "script",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "cache:clear": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true
    }
  }
}
